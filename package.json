{
  "name": "@elizaos/plugin-polymarket",
  "version": "0.1.9",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "agentConfig": {
    "name": "@elizaos/plugin-polymarket",
    "version": "1.0.0",
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "API_KEY": {
        "type": "string",
        "description": "API key for Polymarket service"
      }
    }
  },
  "keywords": [
    "elizaos-plugin",
    "prediction-markets",
    "polymarket"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.10",
    "@elizaos/plugin-0g": "1.0.0"
  },
  "packageType": "plugin",
  "platform": "node",
  "trustedDependencies": [
    "es5-ext"
  ]
}